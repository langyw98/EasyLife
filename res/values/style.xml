<?xml version="1.0" encoding="utf-8"?>
<resources>
		<style name="style1">
		<item name="android:textSize">14sp</item>
		<item name="android:textColor">#000FFF</item>
		<item name="android:layout_marginTop">5dip</item>
		<item name="android:layout_marginLeft">15dip</item>
		</style>
	<style name="main_tab_bottom">
        <item name="android:textSize">10.0dip</item>
        <item name="android:textColor">#919191</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_horizontal</item>  
        <item name="android:background">@drawable/home_btn_bg</item>
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>  
        <item name="android:singleLine">true</item>
        <item name="android:drawablePadding">2.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="textStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:maxLines">1</item>  
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/textcolor</item>
    </style>
    <style name="detailStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/textcolor</item>
    </style>
</resources>
